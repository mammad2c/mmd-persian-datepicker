{
  "name": "react-mmd-persian-datepicker",
  "version": "0.1.107",
  "description": "React component for mmd-persian-datepicker",
  "keywords": [
    "datepicker",
    "calendar",
    "persian",
    "jalali",
    "farsi",
    "persian datepicker",
    "jalali datepicker",
    "persian calendar"
  ],
  "main": "dist/react-mmd-persian-datepicker.umd.js",
  "module": "dist/react-mmd-persian-datepicker.es5.js",
  "typings": "dist/types/ReactComponent.d.ts",
  "files": [
    "dist"
  ],
  "author": "Mohammad Toosi m_dd@rogers.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/mammad2c/mmd-persian-datepicker",
    "directory": "packages/react-mmd-persian-datepicker"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist && rimraf .cache",
    "build": "tsc --module commonjs && cross-env NODE_ENV=production rollup -c rollup.config.ts",
    "start": "yarn prebuild && rollup -c rollup.config.ts -w",
    "example": "parcel example/index.html",
    "test": "jest --coverage",
    "prepublish": "yarn build",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "lint:js": "eslint --fix src/**/*.tsx",
    "lint:styles": "stylelint --fix src/**/*.{css,scss,less,sass}"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts,tsx}"
    ]
  },
  "dependencies": {
    "mmd-persian-datepicker": "^0.1.107",
    "react": "17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/jest": "^26.0.21",
    "@types/moment-jalaali": "^0.7.4",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "jest": "^26.6.3",
    "jest-config": "^26.6.3",
    "parcel": "^2.0.0-beta.2",
    "postcss": "^8.2.8",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.42.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "sass": "^1.32.8",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.19.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  }
}
