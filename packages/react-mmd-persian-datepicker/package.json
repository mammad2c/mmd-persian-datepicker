{
  "name": "react-mmd-persian-datepicker",
  "version": "0.1.107",
  "description": "React component for mmd-persian-datepicker",
  "keywords": [
    "datepicker",
    "calendar",
    "persian",
    "jalali",
    "farsi",
    "persian datepicker",
    "jalali datepicker",
    "persian calendar"
  ],
  "main": "dist/react-mmd-persian-datepicker.umd.js",
  "module": "dist/react-mmd-persian-datepicker.es5.js",
  "typings": "dist/types/ReactComponent.d.ts",
  "files": [
    "dist"
  ],
  "author": "Mohammad Toosi m_dd@rogers.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/mammad2c/mmd-persian-datepicker",
    "directory": "packages/react-mmd-persian-datepicker"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist && rimraf .cache",
    "build": "tsc --module commonjs && cross-env NODE_ENV=production rollup -c rollup.config.ts",
    "start": "yarn prebuild && rollup -c rollup.config.ts -w",
    "start:example": "parcel example/index.html",
    "test": "jest --coverage",
    "prepublish": "yarn build",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "lint": "lint-staged",
    "lint:js": "eslint --fix src/**/*.tsx",
    "lint:styles": "stylelint --fix src/**/*.{css,scss,less,sass}"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint:js"
    ],
    "src/**/*.{css,scss,less,sass}": [
      "yarn lint:styles"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "dependencies": {
    "mmd-persian-datepicker": "^0.1.107",
    "react": "16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/jest": "^26.0.7",
    "@types/moment-jalaali": "^0.7.4",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "jest": "^26.1.0",
    "jest-config": "^26.1.0",
    "lint-staged": "^10.2.11",
    "node-sass": "^4.14.1",
    "parcel": "^1.12.4",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.23.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^3.1.3",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.18.0",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
