{
  "name": "mmd-persian-datepicker",
  "version": "0.1.107",
  "description": "A pure js persian datepicker, powered by TypeScript :)",
  "keywords": [
    "datepicker",
    "calendar",
    "persian",
    "jalali",
    "farsi",
    "persian datepicker",
    "jalali datepicker",
    "persian calendar"
  ],
  "main": "dist/mmd-persian-datepicker.umd.js",
  "module": "dist/mmd-persian-datepicker.es5.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Mohammad Toosi m_dd@rogers.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/mammad2c/mmd-persian-datepicker",
    "directory": "packages/mmd-persian-datepicker"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist && rimraf .cache",
    "build": "tsc --module commonjs && cross-env NODE_ENV=production rollup -c rollup.config.ts",
    "start": "yarn prebuild && rollup -c rollup.config.ts -w",
    "example": "parcel example/index.html",
    "prepublish": "yarn build",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "lint:js": "eslint --fix src/**/*.ts",
    "lint:styles": "stylelint --fix src/**/*.{css,scss,less,sass}"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts,tsx}"
    ]
  },
  "peerDependencies": {
    "moment": "^2.29.1",
    "moment-jalaali": "^0.9.2"
  },
  "dependencies": {
    "moment": "^2.29.1",
    "moment-jalaali": "^0.9.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/moment-jalaali": "^0.7.4",
    "@types/node": "^14.14.32",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-config": "^26.6.3",
    "parcel": "^2.0.0-beta.1",
    "postcss": "^8.2.7",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.40.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "sass": "1.32.8",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.19.0",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  }
}
